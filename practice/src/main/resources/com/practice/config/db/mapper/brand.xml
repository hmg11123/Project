<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="brand">

	<select id="selectList" resultType="brandVO" parameterType="string">
		SELECT 	BRAND_UID,
				NAME,
				TYPE,
				PATH
		  FROM	BRAND
		 WHERE	TYPE =  #{type}
	</select>

	

</mapper>
	